      head.js(
        "http://ajax.googleapis.com/ajax/libs/jquery/1.4.4/jquery.min.js",
        "http://static.baubauhaus.com/javascripts/history.min.js",
        "http://static.baubauhaus.com/javascripts/jquery.plugins.min.js",
        
        "http://static.baubauhaus.com/javascripts/baubauhaus.min.js",
        function() {
          jQuery(document).ready(function() {  layout.init(); });

          head.js(
            "http://www.google-analytics.com/ga.js",
            function() {
              var tracker = _gat._getTracker("UA-1595815-2");
              tracker._setDomainName('www.baubauhaus.com');
              tracker._trackPageview();

              jQuery(document).ready(function() { $(document).ajaxSend(function(event, xhr, settings){ tracker._trackPageview(settings.url); });
            })
          });

        }
      );

      

